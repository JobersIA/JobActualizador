{
  "expo": {
    "name": "JobActualizador",
    "slug": "jobactualizador",
    "version": "1.0.0",
    "scheme": "jobactualizador",
    "orientation": "portrait",
    "icon": "./assets/images/icono_app.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#228B22"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.rsrsystem.jobactualizador"
    },
    "android": {
      "package": "com.rsrsystem.jobactualizador",
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/icono_app.png",
        "backgroundColor": "#228B22"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "permissions": [
        "android.permission.REQUEST_INSTALL_PACKAGES",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.WRITE_EXTERNAL_STORAGE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router",
      "expo-font"
    ],
    "extra": {
      "router": {},
      "eas": {
        "projectId": "ff21a4f5-10c0-4aad-a731-0a9d089b0a23"
      },
      "buildDate": "2025-12-02T12:00:00.000Z"
    },
    "owner": "jobers-soluciones-infomaticas"
  }
}